
@{
    ViewBag.Title = "CompanyListPending";

    Layout = "~/Views/Shared/_LayoutPage2.cshtml";
}


    <div class="row">
        <div class="col-lg-12">
            <section class="panel">
                <header class="panel-heading">
                    <h2 class="panel-title">CompanyList</h2>
                </header>
                <div class="panel-body">
                    <div class="table-responsive">
                        <table class="table table-bordered table-hover mb-none">
                            <thead>
                                <tr>

                                    <th>Username</th>
                                    <th>Email</th>
                                    <th>Mobile</th>
                                    <th>Status</th>
                                    <th>Action</th>

                                </tr>
                            </thead>
                            <tbody>
                                @for (int i = 0; i < Model.Rows.Count; i++)
                                {
                                    <tr>

                                        <td>@Model.Rows[i]["Username"]</td>
                                        <td>@Model.Rows[i]["Email"]</td>
                                        <td>@Model.Rows[i]["Mobile"]</td>
                                        <td>@Model.Rows[i]["Status"]</td>
                                        <td>
                                            <a href="/Admin/CompanyDetails?SID=@Model.Rows[i]["CompanyID"]">Edit</a>

                                        </td>
                                    </tr>
                                }
                            </tbody>
                        </table>

                    </div>
                </div>
            </section>

        </div>
    </div>


